            -ms-flex: 1;
                flex: 1;
        margin-left: 28rpx;
        background: #fff;
        border-radius: 10rpx;
        height: 60rpx;
        line-height: 60rpx;
        padding: 0 20rpx;
        -webkit-box-sizing: border-box;
                box-sizing: border-box;
        font-size: 28rpx;
}
.login .login-wrapper .login-password {
      margin-top: 40rpx;
}
