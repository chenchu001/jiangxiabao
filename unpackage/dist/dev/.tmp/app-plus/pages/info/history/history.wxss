page {
  height: 100%;
}
.uni-media-list-logo, .lazy-image {
  width: 240rpx !important;
  height: 160rpx !important;
  -webkit-box-flex: 0 !important;
  -webkit-flex: 0 0 240rpx !important;
      -ms-flex: 0 0 240rpx !important;
          flex: 0 0 240rpx !important;
  margin-right: 20rpx;
}
.history {
  width: 100%;
  height: 100%;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  position: relative;
}
.history .no-result-wrap {
    width: 160rpx;
    height: 200rpx;
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -80rpx;
    margin-top: -100rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
        -ms-flex-direction: column;
            flex-direction: column;
    -webkit-box-align: center;
    -webkit-align-items: center;
        -ms-flex-align: center;
            align-items: center;
}
.history .no-result-wrap .notice-none {
      width: 160rpx;
      height: 128rpx;
}
.history .no-result-wrap .no-result-text {
      font-size: 28rpx;
      margin-top: 10rpx;
}
.uni-media-list {
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
}
.uni-media-list-body {
  height: auto;
}
.uni-media-list-text-top {
