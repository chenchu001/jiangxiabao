page {
  width: 100%;
  height: 100%;
}
.set {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.set .title {
    margin-top: 140rpx;
    text-align: center;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
