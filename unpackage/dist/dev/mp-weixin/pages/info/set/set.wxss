            flex-direction: column;
    -webkit-box-align: center;
    -webkit-align-items: center;
        -ms-flex-align: center;
            align-items: center;
}
.set .title .logo {
      width: 160rpx;
      height: 160rpx;
}
.set .title .text {
      font-size: 40rpx;
      font-weight: 700;
      margin: 10rpx 0 2rpx;
}
.set .title .version {
      font-size: 24rpx;
      color: #666;
}
